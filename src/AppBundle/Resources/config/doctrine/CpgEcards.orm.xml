<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CpgEcards" table="CPG_ecards">
    <id name="eid" type="integer" column="eid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="senderName" type="string" column="sender_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="senderEmail" type="text" column="sender_email" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="recipientName" type="string" column="recipient_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="recipientEmail" type="text" column="recipient_email" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="link" type="text" column="link" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="text" column="date" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="senderIp" type="text" column="sender_ip" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
